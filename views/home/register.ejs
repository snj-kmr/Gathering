  <script>            
    setTimeout(function(){ 
      function validateForm() {
        var a = document.forms["myForm"]["fname"].value;
        var b = document.forms["myForm"]["lname"].value;
        var c = document.forms["myForm"]["age"].value;
        var d = document.forms["myForm"]["gender"].value;
        var e = document.forms["myForm"]["number"].value;
        var f = document.forms["myForm"]["password"].value;
        var g = document.forms["myForm"]["title"].value;
        if (a == "" || b == "" || c == "" || d == "" || e == "" || f == "" || g == "") {
            alert("Must be filled out");
            return false;}
    }
    function validateForm1() {
        var a = document.forms["myForm"]["cname"].value;
        var c = document.forms["myForm"]["ctype"].value;
        var d = document.forms["myForm"]["cdiscription"].value;
        var e = document.forms["myForm"]["cproduct"].value;
        var f = document.forms["myForm"]["delivery"].value;
        var g = document.forms["myForm"]["role"].value;
        if (a == "" || c == "" || d == "" || e == "" || f == "" || g == "") {
            alert("Must be filled out");
            return false;}
    }
      $(document).ready(function(){
      $("#business_form").css("display","none");
      $("#contact_form").css("display","none");   
      $("#reg_form_button").click(function(){
      document.body.scrollTop = 0;
      document.documentElement.scrollTop = 0;
        var response = validateForm();
        if(response!=false){
            $('#form-tabs p:nth-child(2)').addClass("active");
            $("#business_form").css("display","block");
            $("#reg_form").css("display","none");
            $("#contact_form").css("display","none");
        }        
      })   
       $("#business_form_btn").click(function(){
        document.body.scrollTop = 0;
        document.documentElement.scrollTop = 0;
          var response1 = validateForm1();  
       if(response1!=false){ 
                $('#form-tabs p:nth-child(3)').addClass("active");
                $("#business_form").css("display","none");
                $("#reg_form").css("display","none");
                $("#contact_form").css("display","block");
            }
      })
      $(".btn").on("click", function () {
        $("#aaa").addClass("span4");
        $(".alert").removeClass("in").show();
        $(".alert").delay(200).addClass("in").fadeOut(5000);
      });
      // p tags
      $("#form-tabs p:nth-child(1)").click(function(){
        $('#form-tabs p').removeClass("active");
        $('#form-tabs p:nth-child(1)').addClass("active");
        $("#reg_form").css("display","block");
        $("#business_form").css("display","none");
        $("#contact_form").css("display","none");
      }) 
      $("#form-tabs p:nth-child(2)").click(function(){
        $('#form-tabs p').removeClass("active");
        $('#form-tabs p:nth-child(2)').addClass("active");
        $('#form-tabs p:nth-child(1)').addClass("active");
        $("#reg_form").css("display","none");
        $("#business_form").css("display","block");
        $("#contact_form").css("display","none");
      })
      })
     }, 500);
     setTimeout(function(){
       $(document).ready(function(){
      $('#myForm').validator() 
       })
     }, 1000);
    </script>   
